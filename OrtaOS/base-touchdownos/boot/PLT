#!/bin/bash

# DO NOT EDIT THIS FILE!!
# This is a partition location table file that bootloader reads to correclty locate the system and its file.
# If this file is edited, the system may not load properly.

export NVDEV="/tmp/HARTS"
export ROOTFS="/tmp/HARTS/ortaos/vrootfs"

export SYSTEM="$ROOTFS/System"
export EMUDISK="$ROOTFS/emulated"
export CACHE="$EMUDISK/0"
export DATA="$EMUDISK/1"

export TDLIB="$SYSTEM/TouchDown"
export BOOTSECT="$SYSTEM/boot"
export SBIN="$SYSTEM/sbin"

export USERDATA="$DATA/User"
export LIB="$DATA/Library"
export MULTIPLEX="$DATA/config"

export NVRAM="$LIB/nvram"


# Assigned global variables for files

# OrtaOS Accessible variables

# Bootstage
export BOOTREFUSE="$CACHE/init-boot-refuse"
export FBOOTREFUSE="$CACHE/framework-boot-refuse"
export BOOTREFUSE_CULPRIT="$NVDEV/orta-error"
export BOOTARGS="$NVDEV/bootargs"

# Runtime
export PYTHONLIB="/usr/local/Cellar/python@3.8"
export PYTHON="/usr/local/Cellar/python@3.8/python3.8.4/Frameworks/Python.framework/Versions/3.8/bin/python3.8"
export ORTA="$SYSTEM/Orta"

# Teletype Interface
export TTYIN="$CACHE/tty_in"
export TTYOUT="$CACHE/tty_out"
export ASK_SUPERUSER="$CACHE/ask_superuser"
export ASK_SUPERTOKEN=""